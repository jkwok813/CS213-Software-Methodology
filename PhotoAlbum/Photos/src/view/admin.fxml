<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.adminController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <ListView fx:id="listView" layoutX="215.0" layoutY="19.0" prefHeight="358.0" prefWidth="400.0" />
            <Button layoutX="28.0" layoutY="83.0" mnemonicParsing="false" onAction="#addUser" text="Add User" />
            <Button layoutX="28.0" layoutY="198.0" mnemonicParsing="false" onAction="#delUser" text="Delete User" />
            <TextField fx:id="userinput" layoutX="28.0" layoutY="141.0" promptText="Enter user to be added" />
            <Button layoutX="28.0" layoutY="343.0" mnemonicParsing="false" onAction="#quit" text="Quit" />
            <Button layoutX="95.0" layoutY="343.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      </children>
    </AnchorPane>
  </children>
</VBox>
