<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AlbumController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button layoutX="30.0" layoutY="50.0" mnemonicParsing="false" onAction="#add" text="Add" />
            <Button layoutX="29.0" layoutY="200.0" mnemonicParsing="false" onAction="#rename" text="Rename" />
            <Button layoutX="29.0" layoutY="152.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
            <Button layoutX="29.0" layoutY="345.0" mnemonicParsing="false" onAction="#quit" text="Quit" />
            <TextField fx:id="albums" layoutX="29.0" layoutY="100.0" promptText="Enter Album Name" />
            <ListView fx:id="listView" layoutX="192.0" layoutY="3.0" prefHeight="394.0" prefWidth="242.0" />
            <Button layoutX="29.0" layoutY="300.0" mnemonicParsing="false" onAction="#open" text="Open" />
            <Button layoutX="88.0" layoutY="345.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
            <TextArea fx:id="disp" layoutX="434.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" />
            <Label layoutX="460.0" layoutY="75.0" text="Number of photos in album" />
            <TextField fx:id="albums1" layoutX="29.0" layoutY="250.0" promptText="Enter new album name" />
      </children>
    </AnchorPane>
  </children>
</VBox>
